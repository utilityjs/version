  },
  "exports": "./mod.ts",
  "exclude": [
    "dist"
  ],
  "publish": {
    "exclude": [
      ".github/",
      "test/"
    ]
  },
  "imports": {
    "@std/semver": "jsr:@std/semver@^0.220.1",
    "@utility/git": "jsr:@utility/git@^0.2.0"
  },
  "version": "1.0.0-pre.0"
}
